{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "ordered-imports": [
      false
    ],
    "object-literal-sort-keys": [
      false
    ],
    "no-vars-requires": [
      false
    ],
    "no-console": false,
    "no-debugger": false,
    "object-literal-sort-keys": false
  },
  "jsRules": {
    "object-literal-sort-keys": false, //Disable for javascript
    "no-console": false,
    "no-debugger": false
  }
}